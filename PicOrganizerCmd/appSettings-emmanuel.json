        "Args": {
          "path": "%TEMP%/Logs/PicOrganizer-e-.txt",
          "rollingInterval": "Day"
        }
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ],
    "Properties": {
      "Application": "PicOrganizer"
    }
  }
