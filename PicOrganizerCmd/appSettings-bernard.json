{
  "InputSettings": {
    "SourceFolders": [
      "C:\\PhotoLibrary\\Bernard-source"
    ],
    "CleanDirectoryName": "C:\\PhotoLibrary\\Params\\CleanDirectoryName.csv",
    "TimelineName": "",
    "StartingYearOfLibrary": 1900,
    "ExcludedFiles": [
    ],
    "Scanned": "scanned",
    "Mode": 0
  },
  "OutputSettings": {
    "TargetDirectories": [ "C:\\photolibrary\\Bernard" ],
    "VideosFolderName": "videos",
    "PicturesFolderName": "photos",
    "MetaDataFolderName": "meta-data",
    "DeleteDuplicates": true,
    "DuplicatesFolderName": "duplicates",
    "InvalidJpegFolderName": "invalid-jpeg",
    "UnknownDateFolderName": "date-unknown",
    "UnkownFolderName": "unknown",
    "WhatsappFolderName": "whatsapp",
    "ReportDuplicatesName": "reportDuplicates.csv",
    "ReportDetailName": "reportDetail.csv",
    "SubFolderDateFormat": "yyyy-MM"
  },
  "PictureExtensions": [
    ".jpeg",
    ".jpg",
    ".png",
    ".bmp",
    ".tiff"
  ],
  "VideoExtensions": [
    ".avi",
    ".mpg",
    ".mpeg",
    ".mp4",
    ".mov",
    ".wmv",
    ".mkv"
  ],
  "KnownUsedDateFormatsInNames": [
    "yyyy-MM-dd-HH-mm-ss",
    "yyyy-MM-dd_HH-mm-ss",
    "yyyyMMdd_HHmmss-",
    "yyyyMMddHHmm",
    "yyyyMMdd_HHmmss",
    "yyyyMMdd",
    "MMddyyHHmm",
    "_yyyy-MM-dd-HH-mm-ss",
    "_yyyyMMdd_HHmmss",
    "_yyyyMMdd_HHmmss_ff",
    "_yyyyMMdd",
    "-yyyyMMdd"
  ],
  "MaxDop": 4,
  "TagSkipper": [
    "the",
    "a",
    "with",
    "or",
    "and",
    "for",
    "pre",
    "vzm",
    "img",
    "lg",
    "samsung",
    "cannon",
    "iphpne",
    "dropbox",
    "auto"
  ],
  "WhatsappNameRegex": "IMG-[0-9]{8}-WA[0-9]{4}",
  "AllFileExtensions": "*",
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
    "MinimumLevel": "Debug",
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "File",
        "Args": {
          "path": "%TEMP%/Logs/PicOrganizer-.txt",
          "rollingInterval": "Day"
        }
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ],
    "Properties": {
      "Application": "PicOrganizer"
    }
  }
}